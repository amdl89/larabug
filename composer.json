{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.0",
        "ext-gd": "*",
        "algolia/algoliasearch-client-php": "^3.2",
        "benjamincrozat/laravel-dropbox-driver": "^3.0",
        "bensampo/laravel-enum": "4.2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.5.4",
        "juststeveking/http-status-code": "^1.0",
        "kreait/laravel-firebase": "3.0.0",
        "laravel/fortify": "^1.8",
        "laravel/framework": "^8.0",
        "laravel/scout": "^9.2",
        "laravel/tinker": "^2.0",
        "masbug/flysystem-google-drive-ext": "^1.0.0",
        "org_heigl/ghostscript": "^2.3",
        "spatie/laravel-collection-macros": "^7.4",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/regex": "^1.4.2",
        "spatie/url": "^1.3.5",
        "teamtnt/laravel-scout-tntsearch-driver": "^11.6",
        "tightenco/ziggy": "^1.4",
        "wulfheart/pretty_routes": "^0.3.0",
        "yoelpc4/laravel-cloudinary": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "laravel-shift/blueprint": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}